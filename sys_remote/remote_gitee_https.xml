<?xml version="1.0" encoding="UTF-8"?>
<manifest>



  <!--下载源定义-->
  <!--gitee使用https类型的协议，推送时不方便使用ssh无密推送-->

    <!--我们在github和gitee都包含同名的仓库-->
    <!--开发活跃的资料，在gitee可能不会时刻保持最新，有时可能会有点滞后-->
    <!--将manifest xml中project的remote参数替换为此处定义的remote名，即可切换下载源-->
  
  <!--本manifest自身的地址-->
  <!--旧的主群组，部分无法迁移的仓库使用-->
  <remote fetch="https://gitee.com/wildfireteam" name="Embedfire-old"/>
  
  <!--新的主群组-->
  <remote fetch="https://gitee.com/Embedfire" name="Embedfire"/>

  <!--debugger -->
  <remote fetch="https://gitee.com/Embedfire-debugger" name="Embedfire-debugger"/>

  <!--gd32 -->
  <remote fetch="https://gitee.com/Embedfire-gd32" name="Embedfire-gd32"/>

  <!--gui -->
  <remote fetch="https://gitee.com/Embedfire-emwin" name="Embedfire-emwin"/>
  <remote fetch="https://gitee.com/Embedfire-emxgui" name="Embedfire-emxgui"/>

  <!--imxrt -->
  <remote fetch="https://gitee.com/Embedfire-rt1052" name="Embedfire-rt1052"/>
  <remote fetch="https://gitee.com/Embedfire-rt1021" name="Embedfire-rt1021"/>

  <!--linux -->
  <remote fetch="https://gitee.com/Embedfire-imx6" name="Embedfire-imx6"/>

  <!--module -->
  <!--camera -->
  <remote fetch="https://gitee.com/Embedfire-camera" name="Embedfire-camera"/>
  <!--display -->
  <remote fetch="https://gitee.com/Embedfire-display" name="Embedfire-display"/>
  <!--gps -->
  <remote fetch="https://gitee.com/Embedfire-gps" name="Embedfire-gps"/>
  <!--gsm -->
  <remote fetch="https://gitee.com/Embedfire-gsm" name="Embedfire-gsm"/>
  <!--misc -->
  <remote fetch="https://gitee.com/Embedfire-misc" name="Embedfire-misc"/>
  <!--sensor -->
  <remote fetch="https://gitee.com/Embedfire-sensor" name="Embedfire-sensor"/>
  <!--wireless -->
  <remote fetch="https://gitee.com/Embedfire-wireless" name="Embedfire-wireless"/>
  <!--ADC_DAC -->
  <remote fetch="https://gitee.com/embedfire-adc-dac" name="Embedfire-adc-dac"/>



 

  <!--network-->
  <remote fetch="https://gitee.com/Embedfire-lwip" name="Embedfire-lwip"/>
  <remote fetch="https://gitee.com/Embedfire-WiFi-AP" name="Embedfire-WiFi-AP"/>
  <remote fetch="https://gitee.com/Embedfire-WiFi-esp" name="Embedfire-WiFi-esp"/>

  <!--rtos -->
  <remote fetch="https://gitee.com/Embedfire-freertos" name="Embedfire-freertos"/>
  <remote fetch="https://gitee.com/Embedfire-ucos" name="Embedfire-ucos"/>
  <remote fetch="https://gitee.com/Embedfire-rtthread" name="Embedfire-rtthread"/>
  <remote fetch="https://gitee.com/Embedfire-liteos" name="Embedfire-liteos"/>




  <!--stm32开发板 -->
  <remote fetch="https://gitee.com/Embedfire-stm32f103-mini" name="Embedfire-stm32f103-mini"/>
  <remote fetch="https://gitee.com/Embedfire-stm32f103-zhinanzhe" name="Embedfire-stm32f103-zhinanzhe"/>
  <remote fetch="https://gitee.com/Embedfire-stm32f103-badao" name="Embedfire-stm32f103-badao"/>
  <remote fetch="https://gitee.com/Embedfire-stm32f407-batianhu" name="Embedfire-stm32f407-batianhu"/>
  <remote fetch="https://gitee.com/Embedfire-stm32f429-tiaozhanzhe" name="Embedfire-stm32f429-tiaozhanzhe"/>
  <remote fetch="https://gitee.com/Embedfire-stm32f767-tiaozhanzhe" name="Embedfire-stm32f767-tiaozhanzhe"/>
  <remote fetch="https://gitee.com/Embedfire-stm32h743-tiaozhanzhe" name="Embedfire-stm32h743-tiaozhanzhe"/>
  <remote fetch="https://gitee.com/Embedfire-stm32h7-pro" name="Embedfire-stm32h7-pro"/>





  <!--stm32电机系列 -->
