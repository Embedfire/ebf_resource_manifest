<?xml version="1.0" encoding="UTF-8"?>
<manifest>



  <!--下载源定义-->
  <!--gitee使用ssh类型的协议，下载时必须加载ssh密钥，不方便直接下载-->

    <!--我们在github和gitee都包含同名的仓库-->
    <!--开发活跃的资料，在gitee可能不会时刻保持最新，有时可能会有点滞后-->
    <!--将manifest xml中project的remote参数替换为此处定义的remote名，即可切换下载源-->
  
  <!--本manifest自身的地址-->
  <remote fetch="ssh://gitee.com/Embedfire" name="Embedfire"/>

  <!--debugger -->
  <remote fetch="ssh://gitee.com/Embedfire-debugger" name="Embedfire-debugger"/>

  <!--gd32 -->
  <remote fetch="ssh://gitee.com/Embedfire-gd32" name="Embedfire-gd32"/>

  <!--gui -->
  <remote fetch="ssh://gitee.com/Embedfire-emwin" name="Embedfire-emwin"/>
  <remote fetch="ssh://gitee.com/Embedfire-emxgui" name="Embedfire-emxgui"/>

  <!--imxrt -->
  <remote fetch="ssh://gitee.com/Embedfire-rt1052" name="Embedfire-rt1052"/>
  <remote fetch="ssh://gitee.com/Embedfire-rt1021" name="Embedfire-rt1021"/>

  <!--linux -->
  <remote fetch="ssh://gitee.com/Embedfire-imx6" name="Embedfire-imx6"/>

  <!--module -->
  <!--camera -->
  <remote fetch="ssh://gitee.com/Embedfire-camera" name="Embedfire-camera"/>
  <!--display -->
  <remote fetch="ssh://gitee.com/Embedfire-display" name="Embedfire-display"/>
  <!--gps -->
  <remote fetch="ssh://gitee.com/Embedfire-gps" name="Embedfire-gps"/>
  <!--gsm -->
  <remote fetch="ssh://gitee.com/Embedfire-gsm" name="Embedfire-gsm"/>
  <!--misc -->
  <remote fetch="ssh://gitee.com/Embedfire-misc" name="Embedfire-misc"/>
  <!--sensor -->
  <remote fetch="ssh://gitee.com/Embedfire-sensor" name="Embedfire-sensor"/>
  <!--wireless -->
  <remote fetch="ssh://gitee.com/Embedfire-wireless" name="Embedfire-wireless"/>

  <!--motor -->
  <remote fetch="ssh://gitee.com/Embedfire-motor" name="Embedfire-motor"/>

  <!--network-->
  <remote fetch="ssh://gitee.com/Embedfire-lwip" name="Embedfire-lwip"/>
  <remote fetch="ssh://gitee.com/Embedfire-WiFi-AP" name="Embedfire-WiFi-AP"/>
  <remote fetch="ssh://gitee.com/Embedfire-WiFi-esp" name="Embedfire-WiFi-esp"/>

  <!--rtos -->
  <remote fetch="ssh://gitee.com/Embedfire-freertos" name="Embedfire-freertos"/>
  <remote fetch="ssh://gitee.com/Embedfire-ucos" name="Embedfire-ucos"/>
  <remote fetch="ssh://gitee.com/Embedfire-rtthread" name="Embedfire-rtthread"/>
  <remote fetch="ssh://gitee.com/Embedfire-liteos" name="Embedfire-liteos"/>

  <!--stm32开发板 -->
  <remote fetch="ssh://gitee.com/Embedfire-stm32f103-mini" name="Embedfire-stm32f103-mini"/>
  <remote fetch="ssh://gitee.com/Embedfire-stm32f103-zhinanzhe" name="Embedfire-stm32f103-zhinanzhe"/>
  <remote fetch="ssh://gitee.com/Embedfire-stm32f103-badao" name="Embedfire-stm32f103-badao"/>
  <remote fetch="ssh://gitee.com/Embedfire-stm32f407-batianhu" name="Embedfire-stm32f407-batianhu"/>
  <remote fetch="ssh://gitee.com/Embedfire-stm32f429-tiaozhanzhe" name="Embedfire-stm32f429-tiaozhanzhe"/>
  <remote fetch="ssh://gitee.com/Embedfire-stm32f767-tiaozhanzhe" name="Embedfire-stm32f767-tiaozhanzhe"/>
  <remote fetch="ssh://gitee.com/Embedfire-stm32h743-tiaozhanzhe" name="Embedfire-stm32h743-tiaozhanzhe"/>
  <remote fetch="ssh://gitee.com/Embedfire-stm32h7-pro" name="Embedfire-stm32h7-pro"/>



</manifest>
